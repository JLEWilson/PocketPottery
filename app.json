{
  "expo": {
    "name": "Pocket Pottery",
    "slug": "pocketpottery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#B7CBB5"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app uses your chosen photos to use on your pottery display."
        }
      ],
      "expo-sqlite",
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#B7CBB5"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jlewilson.pocketpottery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "20b86be5-0977-40ce-b957-52066e56fbd2"
      }
    },
    "expo": {
      "splash": {
        "image": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "newArchEnabled": true,
    "owner": "jlewilson"
  }
}
